{
  "name": "test-css-module",
  "private": true,
  "scripts": {
    "start": "meteor run"
  },
  "dependencies": {
    "antd-mobile": "^2.2.0",
    "indexof": "0.0.1",
    "@babel/runtime": "^7.0.0-beta.42",
    "less": "^2.7.3",
    "meteor-node-stubs": "^0.3.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.2",
    "postcss-modules-extract-imports": "^1.1.0",
    "postcss-modules-local-by-default": "^1.2.0",
    "postcss-modules-scope": "^1.1.0",
    "postcss-modules-values": "^1.3.0",
    "postcss-nested": "^3.0.0",
    "postcss-pxtorem": "^4.0.1",
    "postcss-simple-vars": "^4.1.0"
  },
  "cssModules": {
    "postcssPlugins": {
      "postcss-pxtorem": {
        "inlineOptions": {
          "rootValue": 37.5,
          "propWhiteList": []
        }
      },
      "postcss-simple-vars": {
        "inlineOptions": {}
      },
      "postcss-modules-local-by-default": {},
      "postcss-modules-extract-imports": {},
      "postcss-modules-scope": {}
    },
    "extensions": [
      "less",
      "css"
    ],
    "enableLessCompilation": [
      "less"
    ],
    "passthroughPaths": [
      "client/antd.less"
    ],
    "globalVariables": [],
    "includePaths": [],
    "ignorePaths": [
      "node_modules"
    ]
  }
}
