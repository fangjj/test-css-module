{
  "name": "test-css-module",
  "private": true,
  "scripts": {
    "start": "meteor run"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.42",
    "antd-mobile": "^2.2.0",
    "indexof": "0.0.1",
    "less": "^2.7.3",
    "meteor-node-stubs": "^0.3.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "postcss": "^6.0.22",
    "postcss-load-config": "^1.2.0",
    "postcss-pxtorem": "^4.0.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      },
      "postcss-pxtorem": {
        "rootValue": 37.5,
        "propWhiteList": []
      }
    }
  },
  "cssModules": {
    "extensions": [
      "less",
      "css"
    ],
    "enableLessCompilation": [
      "less"
    ],
    "passthroughPaths": [
      "client/antd.less"
    ],
    "globalVariables": [],
    "includePaths": [],
    "ignorePaths": [
      "node_modules"
    ]
  }
}
